{
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "type": "AdaptiveCard",
    "version": "1.0",
    "body": [
      {
        "type": "TextBlock",
        "size": "Medium",
        "weight": "Bolder",
        "id": "Title",
        "text": "Where are you from?",
        "wrap": true,
        "horizontalAlignment": "Left"
      },
      {
        "type": "Input.Text",
        "id": "acOrigin",
        "placeholder": "Boston, MA"
      }
    ],
    "actions": [
      {
        "type": "Action.Execute",
        "title": "Next Question",
        "data": { "questionNumber" : 4},
        "verb": "nextQuestion"
      }
    ]
  }